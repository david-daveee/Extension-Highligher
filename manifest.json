{
    "manifest_version":2,
    "name":"MEGA Courses Highlighter",
    "description":"Extention to highlight courses in main folder",
    "version":"1.0",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
        "default_popup": "popup.html"
      },
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js": ["contentScripts.js"]
    }]
}